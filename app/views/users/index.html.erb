<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= render 'devise/shared/sidemenu', user: @user_id, newbook: @newbook %>
    </div>
    <div class="col-md-9">

      <%= render 'devise/shared/validation_error', book: @newbook %>
      
      <h2>Users</h2>
      <table>
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_profile_image(100, 100) %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user.id) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
      
 




